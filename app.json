{
  "expo": {
    "name": "vivemas",
    "slug": "vivemas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSHealthShareUsageDescription": "Se utilizarán tus datos de salud para mostrar pasos y calorías.",
        "NSHealthUpdateUsageDescription": "Se utilizarán tus datos de salud para mostrar y actualizar información relacionada con pasos y calorías."
      }
    },
    "android": {
      "permissions": [
        "ACTIVITY_RECOGNITION",
        "BODY_SENSORS",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.tekisolutions.vivemasApp"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a6f34c77-bfc8-4914-9900-9383f4f74eab"
      }
    }
  }
}
